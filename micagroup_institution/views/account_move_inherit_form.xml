<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="private_institution_account_move_inscription_contract" model="ir.ui.view">
            <field name="name">private_institution_account_move_inscription_contract</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@id='header_left_group']//field[@name='partner_id']" position="after">
                    <field name="contract_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>